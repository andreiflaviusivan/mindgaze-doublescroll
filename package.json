{
  "name": "mindgaze-doublescroll",
  "version": "1.0.6",
  "description": "Enable double scroll bars for any Angular 4+ element",
  "main": "index.js",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/mindgaze-doublescroll.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/mindgaze-doublescroll.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreiflaviusivan/mindgaze-doublescroll.git"
  },
  "keywords": [
    "double",
    "scroll",
    "bar",
    "angular4",
    "angular5"
  ],
  "author": "Andrei-Flavius Ivan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/andreiflaviusivan/mindgaze-doublescroll/issues"
  },
  "homepage": "https://github.com/andreiflaviusivan/mindgaze-doublescroll#readme",
  "dependencies": {
    "@angular/common": "^5.0.2",
    "@angular/core": "^5.0.2",
    "jquery": "^3.2.1",
    "rxjs": "^5.5.0",
    "typescript": "2.4.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.5.2",
    "@angular/compiler": "^5.0.2",
    "@angular/compiler-cli": "^5.0.2",
    "@types/jquery": "^3.2.1",
    "rollup": "^0.51.8",
    "typescript": "2.4.2",
    "uglify-js": "^3.1.10"
  }
}
